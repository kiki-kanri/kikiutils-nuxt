{
  "name": "@kikiutils/nuxt",
  "version": "2.0.0-rc.23",
  "description": "Nuxt module.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-nuxt.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "types": "./dist/types.d.ts",
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run build && changelogen --push --release && npm publish"
  },
  "dependencies": {
    "@kikiutils/types": "^3.2.2",
    "@nuxt/kit": "^3.15.4",
    "@unocss/nuxt": "^66.0.0",
    "@vueuse/nuxt": "^12.7.0",
    "defu": "^6.1.4",
    "nuxt": "^3.15.4",
    "sass": "^1.85.0",
    "type-fest": "^4.35.0",
    "vite-plugin-remove-console": "^2.2.0",
    "vue-tsc": "^2.2.2"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.7.1",
    "@kikiutils/eslint-config": "^0.7.4",
    "@kikiutils/node": "^5.1.5",
    "@kikiutils/prettierrc": "^1.3.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/robots": "^5.2.4",
    "copy-to-clipboard": "^3.3.3",
    "element-plus": "^2.9.4",
    "nuxt-purgecss": "^2.0.0",
    "nuxt-security": "^2.1.5",
    "typescript": "~5.6.3"
  },
  "prettier": "@kikiutils/prettierrc",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
